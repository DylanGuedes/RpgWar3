<% provide(:title, 'Sign Up') %>
<center><h1>Sign Up</h1></center>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= render 'fields', f: f %>
      <center><%= f.submit "Done!", class: "button success round large" %></center>
    <% end %>
  </div>
</div>
